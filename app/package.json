{
  "name": "chat-bot-app",
  "version": "0.2.0",
  "description": "discord bot",
  "author": "Hung Nguyen",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "yarn clean",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint src",
    "dev": "nodemon src/main.ts",
    "start": "node build/src/main.js",
    "docker-build": "docker-compose build",
    "docker-dev": "docker-compose -f compose.yaml -f compose.development.yaml up -d",
    "docker-prod": "docker-compose -f compose.yaml -f compose.production.yaml up -d"
  },
  "engines": {
    "node": "20",
    "yarn": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^3.20.0",
    "@google-cloud/vertexai": "^1.2.0",
    "@google/generative-ai": "^0.9.0",
    "@reduxjs/toolkit": "^2.2.3",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "cron": "^3.1.7",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "firebase-admin": "^12.1.1",
    "google-auth-library": "^9.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^9.1.1",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1"
  }
}
